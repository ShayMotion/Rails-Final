<div class="title-container">
<h1>Add an Auction</h1>
</div> 
<%= form_for @auction do |f| %>
<div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
    
    <%= f.label :start_date %>
    <%= f.date_field :start_date %>

    <%= f.label :end_date %>
    <%= f.date_field :end_date %>

    <br></br>

    <%= f.submit%>

    <% end %>