<div class="title-container">
<center><h1>All Guitars</h1>
</div> 

<ul>
<% @guitar.each do |guitar | %>
    <li>
    <%= @guitar.brand %> 
    <%= @guitar.model %> 
    <%= @guitar.year %>
    <%= @guitar.price %>
    <%= f.hidden_field :guitar_id %>

    added by: <%= guitar.user.username %>
    <%= link_to "See all auctions", guitar_auctions_path(guitar)%>
    <%= link_to "Add a guitar", new_guitar_auction_path(guitar)%>
    </li>
    
    <%= render :partial => "guitars/new", :locals => {:guitars => @guitars.brand}%>

    <% end %>
    </ul>